OUTPUT_ARCH(mips)

/**** Start point ****/
ENTRY(__reset_handler)

SECTIONS
{
  /**** Code and read-only data ****/

  .text_init 0x9fc10000 :
  {
    _ftext_init = ABSOLUTE(.);
    init.o (.text)		/* Reset entry point		    */
    init_platform_s.o (.text)
    init_core_s.o (.text)
    atlas_malta_platform.o (.text)
    gt64120_core.o (.text)
    bonito64_core.o (.text)
    msc01_core.o (.text)
    socitsc_core.o (.text)
    dmc_rocit2.o (.text)
    sead_platform.o (.text)
    init_cpu_s.o (.text)
    cache_cpu.o (.text)
    . = ALIGN(8);
    _etext_init = ABSOLUTE(.);
  } = 0

  .text_ram 0x80005000 :
  AT( _etext_init )
  {
    _ftext_ram = ABSOLUTE(.);
    * (.text)
    * (.text.*)
    *(*.MIPS.abiflags*)
    . = ALIGN(8);
    _etext_ram = ABSOLUTE(.);
  } = 0

  /**** Initialised data ****/

  _etext = _etext_ram - _ftext_ram + _etext_init;

  .data _etext_ram :
  AT( _etext )
  {
    _fdata = ABSOLUTE(.);	/* Start of initialised data	    */
    *(.rodata)
    *(.rodata.*)
    *(.data)
    . = ALIGN(8);

    _gp = ABSOLUTE(. + 0x7ff0); /* Base of small data		    */

    *(.lit8) 
    *(.lit4) 
    *(.sdata) 

    . = ALIGN(8);

    _edata  = ABSOLUTE(.);	/* End of initialised data	    */
  }

  /**** Uninitialised data ****/

  _fbss = .;			/* Start of unitialised data	    */

  .sbss : 
  { 
    *(.sbss) 
    *(.scommon)
  }
  .bss :
  {
    *(.bss)
    *(COMMON)
  }

  _end = . ;		/* End of unitialised data	    */

  .gptab.sdata : { *(.gptab.data) *(.gptab.sdata) }
  .gptab.sbss : { *(.gptab.bss) *(.gptab.sbss) }

  /DISCARD/ :
  {
    *(.reginfo)
  }

  _freemem = .;
}


